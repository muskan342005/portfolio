{
  "builds": [
    {
      "src": "myportfolio/wsgi.py",
      "use": "@vercel/python",
      "config": { 
        "maxLambdaSize": "15mb", 
        "runtime": "python3.9",
        "buildCommand": "python vercel_build_script.py"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "myportfolio/wsgi.py"
    }
  ]
}