{
  "version": 2,
  "builds": [
    {
      "src": "myportfolio/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb",
        "runtime": "python3.10"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/myportfolio/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "myportfolio/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "myportfolio.settings"
  }
}
